[retriever]
type = SimpleIMAPSSLRetriever
server = mail.example.com
username = pdf@example.com
password = mypassword

[options]
delete = true

[destination]
type = MDA_external
path = /where/I/installed/email2pdf2
arguments = ("-d", "/where/I/want/PDFs/to/go", "--enforce-syslog", "--mostly-hide-warnings")
